target 'FastBike' do
  use_frameworks!
  platform :ios, '10.0'
  pod 'Alamofire', '~> 4.0'
  pod 'Gloss', :git => 'https://github.com/hkellaway/Gloss', :branch => 'swift_3.0'
  pod 'Google/Analytics'
end

target 'FastBikeWatchOS Extension' do
    use_frameworks!
    platform :watchos, '3.0'
    pod 'Alamofire', '~> 4.0'
    pod 'Gloss', :git => 'https://github.com/hkellaway/Gloss', :branch => 'swift_3.0'
end
